<%@ page language="java" contentType="text/html; charset=US-ASCII"
         pageEncoding="US-ASCII"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=US-ASCII">
        <title>Register for the Feed Generator Service</title>
        <link href="css/jquery-ui.css" rel="stylesheet" type="text/css"/>
        <link href="css/jquery.ui.datepicker.css" rel="stylesheet" type="text/css"/>
        <script type="text/javascript" src="scripts/jquery.min.js"></script>
        <script type="text/javascript" src="scripts/jquery-ui-1.10.4.custom.min.js"></script>
        <script type="text/javascript" src="scripts/jquery.ui.core.min.js"></script>
        <script type="text/javascript" src="scripts/jquery.ui.widget.min.js"></script>
        <script type="text/javascript" src="scripts/jquery.ui.datepicker.min.js"></script>
        <script type="text/javascript" src="scripts/parameters.js"></script>
        <script type="text/javascript">
            $(function() {
                $( "#from" ).datepicker({
                    defaultDate: "+1w",
                    changeMonth: true,
                    numberOfMonths: 3,
                    onClose: function( selectedDate ) {
                        $( "#to" ).datepicker( "option", "minDate", selectedDate );
                    }
                });
                
                $( "#to" ).datepicker({
                    defaultDate: "+1w",
                    changeMonth: true,
                    numberOfMonths: 3,
                    onClose: function( selectedDate ) {
                        $( "#from" ).datepicker( "option", "maxDate", selectedDate );
                    }
                });
            });
        </script>
        <link href="css/view.css" rel="stylesheet" type="text/css"/>
    </head>
    <body style="margin: 0 auto">
        <%
            
             String userName = null;
             Cookie[] cookies = request.getCookies();
             if(cookies !=null){
                for(Cookie cookie : cookies){
                   if(cookie.getName().equals("user")) userName = cookie.getValue();
                }
             }
             if(userName == null) response.sendRedirect("index.jsp");
        %>
        <br>
        <h1>All form fields are mandatory</h1>

        <form id="form_804346" class="appnitro"  method="post" action="">						
            <ul >
		<li id="li_4" >
                    <label class="description" for="element_4">Contact No.</label>
                    <div>
                        <input id="element_4" name="element_4" class="element text medium" type="text" maxlength="255" value=""/> 
                    </div> 
                </li>
                <li id="li_5" >
                    <label class="description" for="element_5">Content Period (Months) </label>
                    <div>
                        <input id="from" name="element_5" class="hasDatePicker" type="text"  value=""/> to 
                        <input id="to" name="element_9" class="hasDatePicker" type="text" value=""/>
                    </div> 
                </li>
                <li id="li_9" >
                    <label class="description" for="element_9">Frequency of Feed (in millisec)</label>
                    <div>
                        <input id="element_9" name="element_9" class="element text medium" type="text" maxlength="255" value=""/>
                    </div> 
                </li>
                <li id="li_6" >
                    <label class="description" for="element_6">Requested Destination Address (Public IP address) </label>
                    <div>
                        <input id="element_6" name="element_6" class="element text medium" type="text" maxlength="255" value=""/> 
                    </div> 
                </li>
                <li id="li_7" >
                    <label class="description" for="element_7">Requested Destination Port </label>
                    <div>
                        <input id="element_7" name="element_7" class="element text medium" type="text" maxlength="255" value=""/> 
                    </div> 
                </li>
                <li id="li_8" >
                    <label class="description" for="element_8">Type of Log Files</label>
                    <div>
                        <select class="element select medium" id="element_8" name="element_8">
                            <option value="1" >Sharepoint</option>
                            <option value="2" >Proxy</option>
                            <option value="3" >Vontu</option>
                            <option value="4" >Windows</option>
                        </select>
                    </div> 
                </li>

                <li class="buttons">
                    <input type="hidden" name="form_id" value="804346" />

                    <button id="saveForm" class="button_text" type="submit" >Submit</button>
                </li>
            </ul>
        </form>	

        <form action="LogoutServlet" method="post">
            <input type="submit" value="Logout" >
        </form>

        <div id="errormessages">

        </div>
    </body>
</html>
