<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>authenticate</servlet-name>
        <servlet-class>com.feedgeneratorui.authenticate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateDBWithChoices</servlet-name>
        <servlet-class>com.feedgeneratorui.updateDBWithChoices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThreadManagement</servlet-name>
        <servlet-class>com.feedgeneratorui.ThreadManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreviewUsersFile</servlet-name>
        <servlet-class>com.feedgeneratorui.PreviewUsersFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>authenticate</servlet-name>
        <url-pattern>/authenticate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateDBWithChoices</servlet-name>
        <url-pattern>/updateDBWithChoices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThreadManagement</servlet-name>
        <url-pattern>/ThreadManagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreviewUsersFile</servlet-name>
        <url-pattern>/PreviewUsersFile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
